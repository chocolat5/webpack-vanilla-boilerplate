{
  "name": "jquery-boilerplate",
  "version": "1.0.0",
  "description": "Webpack 4 boilerplate: Babel, Sass, hot reload and dev server",
  "private": true,
  "author": "chocolat",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "lint": "eslint src/js/*.js",
    "buildall:build": "webpack --config webpack.prod.js",
    "buildall:format": "prettier --write 'dist/assets/css/*.css'",
    "buildall": "npm-run-all buildall:*"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "gsap": "^2.1.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss-cli": "^6.1.3",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^7.3.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "jquery": "^3.4.1"
  },
  "browserslist": [
    "defaults",
    "> 1%",
    "not op_mini all"
  ]
}
